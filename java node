import java.awt.Color;
import java.awt.Graphics;


public class Node
{
	int locx = 0;
	int locy = 0;
	int width = 0;
	int height = 0;
	public void setLocation(int XIN, int YIN, int WIN, int HIN)
	{
		locx = XIN;
		locy = YIN;
		width = WIN;
		height = HIN;
	}
	public int getX()
	{
		return locx;
	}
	public int getY()
	{
		return locy;
	}
	public int getWidth()
	{
		return width;
	}
	public int getHeight()
	{
		return height;
	}
	public void paintMe(Graphics G)
	{
		G.setColor(Color.black);
		G.fillOval(locx, locy, width, height);
	}
}
